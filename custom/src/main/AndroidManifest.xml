<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qinlangchan.custom">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-library android:name="android.test.runner" />

        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GomokuActivity" />
        <activity android:name=".LuckyPanelActivity" />
        <activity android:name=".ProgressActivity" />
        <activity android:name=".recycler.RecyclerActivity" />
        <activity android:name=".GuaGuaActivity" />
        <activity android:name=".chat.ChatActivity" />
        <activity android:name=".FlowLayoutActivity" />
        <activity android:name=".ZoomActivity" />
        <activity android:name=".PuzzleActivity"></activity>
    </application>

</manifest>